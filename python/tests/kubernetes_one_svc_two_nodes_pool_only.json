{
  "resources": {
    "virtualServers": [
      {
        "name": "default_configmap",
        "partition": "k8s",
        "mode": "http",
        "pool": "/k8s/default_configmap",
        "balance": "round-robin"
      }
    ],
    "pools": [
      {
        "name": "default_configmap",
        "partition": "k8s",
        "poolMemberAddrs": [
          "172.16.0.5:30008",
          "172.16.0.6:30008"
        ],
        "serviceName": "foo",
        "servicePort": 80,
        "monitor": [
          "/k8s/default_configmap",
          "/k8s/default_configmap_1"
        ]
      }
    ],
    "monitors": [
      {
        "name": "default_configmap",
        "partition": "k8s",
        "interval": 30,
        "protocol": "tcp",
        "send": "GET /",
        "timeout": 20
      }, {
        "name": "default_configmap_1",
        "partition": "k8s",
        "interval": 10,
        "protocol": "http",
        "send": "GET /",
        "timeout": 5
      }
    ]
  }
}
